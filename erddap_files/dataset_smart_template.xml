<dataset type="EDDTableFromMultidimNcFiles" datasetID="backyardbuoys_template_smart" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/home/stravis/backyardbuoys_data/</fileDir>
    <fileNameRegex>bb_regex_.*\.nc</fileNameRegex>
    <recursive>false</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <removeMVRows>true</removeMVRows>
    <sortFilesBySourceNames></sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <defaultDataQuery>        
        time,latitude,longitude,sea_water_temperature,depth&amp;time&gt;=max(time)-30days
    </defaultDataQuery>
    <defaultGraphQuery>
	time,sea_water_temperature,depth
        &amp;time&gt;=max(time)-30days
	&amp;.draw=markers
        &amp;.vars=time|sea_water_temperature|depth
        &amp;.yRange=||true
        &amp;.marker=11|3
    </defaultGraphQuery>
    <addAttributes>
        <att name="testOutOfDate">now-30days</att>
        <att name="_NCProperties">null</att>
        <att name="Conventions">CF-1.10, ACDD-1.3, IOOS-1.2, COARDS</att>
        <att name="keywords">agg, aggregate, aggregate_quality_flag, association, backyardbuoys, buoy, buoy_id, change, data, density, direction, directional, earth, earth science &amp;gt, Earth Science &gt; Oceans &gt; Ocean Temperature &gt; Sea Surface Temperature, Earth Science &gt; Oceans &gt; Ocean Waves &gt; Significant Wave Height, Earth Science &gt; Oceans &gt; Ocean Waves &gt; Wave Period, Earth Science &gt; Oceans &gt; Ocean Waves &gt; Wave Spectra, Earth Science &gt; Oceans &gt; Ocean Waves &gt; Wave Speed/Direction, flag, flat, flat_line_test_quality_flag, foundation, gross, gross_range_test_quality_flag, height, identifier, latitude, line, location_id, longitude, maximum, mean, source, spectra, spectral, speed, spike, spike_test_quality_flag, spread, station, statistics, surface, surface waves, systems, temperature, test, tests, time, variance, wave, wave period, wave spectra, wave speed/direction, waves, with</att>
        <att name="sourceUrl">(local files)</att>
        <att name="cdm_timeseries_variables">location_id, latitude, longitude</att>
        <att name="subsetVariables">buoy_id</att>
    </addAttributes>
    <dataVariable>
        <sourceName>location_id</sourceName>
        <destinationName>location_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="long_name">Location Id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>time</sourceName>
        <destinationName>time</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="ioos_category">Time</att>
            <att name="units">seconds since 1970-01-01T00:00:00Z</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>buoy_id</sourceName>
        <destinationName>buoy_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="long_name">Buoy Id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>latitude</sourceName>
        <destinationName>latitude</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">90.0</att>
            <att name="colorBarMinimum" type="double">-90.0</att>
            <att name="long_name">Latitude</att>
            <att name="units">degrees_north</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>longitude</sourceName>
        <destinationName>longitude</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">180.0</att>
            <att name="colorBarMinimum" type="double">-180.0</att>
            <att name="long_name">Longitude</att>
            <att name="units">degrees_east</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>depth</sourceName>
        <destinationName>depth</destinationName>
        <dataType>double</dataType>
        <addAttributes>
            <att name="colorBarMaximum" type="double">100</att>
            <att name="colorBarMinimum" type="double">0</att>
            <att name="units">meters</att>
            <att name="ioos_category">Depth</att>
            <att name="long_name">Depth</att>
            <att name="standard_name">depth</att>
        </addAttributes>
    </dataVariable>
</dataset>


